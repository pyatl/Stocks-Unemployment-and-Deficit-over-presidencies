    "## Whatif"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "def whatif(buydate, selldate):\n",
    "    start_price = yf.download(\"SPY\", start=buydate, end=buydate+td(days=5)).iloc[0]['Open']\n",
    "    end_price = yf.download(\"SPY\", start=selldate, end=selldate+td(days=5)).iloc[0]['Open']\n",
    "    print(str(end_price/start_price*100)+\"% earnings\")"
   ]
  },
