    "# Unemployment\n",
    "\n",
    "Source: Civilian Unemployment Rate from the U.S. Bureau of Labor Statistics\n",
    "https://www.bls.gov/charts/employment-situation/civilian-unemployment-rate.htm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Date\n",
       "2000-08-01    4.1\n",
       "2000-09-01    3.9\n",
       "2000-10-01    3.9\n",
       "2000-11-01    3.9\n",
       "2000-12-01    3.9\n",
       "Name: Unemployment, dtype: float64"
      ]
     },
     "execution_count": 10,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "unemployment = pd.read_csv(\"./BLS.csv\", index_col=\"Month\", parse_dates=True, infer_datetime_format=True)['Total']\n",
    "unemployment.index = pd.to_datetime(unemployment.index, format=\"%b-%y\")\n",
    "unemployment.name = \"Unemployment\"\n",
    "unemployment.index.name = \"Date\"\n",
    "unemployment.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "bush_unemployment_mask = (unemployment.index >= bush_inaguration) & (unemployment.index < obama_inaguration)\n",
    "obama_unemployment_mask = (unemployment.index >= obama_inaguration) & (unemployment.index < trump_inaguration)\n",
    "trump_unemployment_mask = (unemployment.index >= trump_inaguration)\n",
    "covid_mask = (unemployment.index >= covid_begins)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "unemployment = pd.DataFrame(unemployment)\n",
    "unemployment['President'] = \"\"\n",
    "unemployment[\"COVID\"] = covid_mask\n",
    "unemployment.loc[bush_unemployment_mask, \"President\"] = \"Bush\"\n",
    "unemployment.loc[obama_unemployment_mask, \"President\"] = \"Obama\"\n",
    "unemployment.loc[trump_unemployment_mask, \"President\"] = \"Trump\"\n",
    "unemployment = unemployment[unemployment.index > bush_inaguration]\n",
    "unemployment = unemployment.reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "bush_unemployment = unemployment[unemployment[\"President\"]==\"Bush\"]\n",
    "obama_unemployment = unemployment[unemployment[\"President\"]==\"Obama\"]\n",
    "trump_unemployment = unemployment[unemployment[\"President\"]==\"Trump\"]\n",
    "trump_unemployment_precovid = unemployment[(unemployment['COVID']==False) & (unemployment[\"President\"]==\"Trump\")]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "def plot_trendline0(df, ax, *, color, label):\n",
    "    x = np.array(df['Date'].map(datetime.date.toordinal))\n",
    "    slope, y0, r, p, stderr = stats.linregress(x,\n",
    "                                               df['Unemployment'])\n",
    "    x_endpoints = pd.DataFrame([x[0], x[-1]])\n",
    "    y_endpoints = y0 + slope * x_endpoints\n",
    "    ax.semilogy(x_endpoints, y_endpoints, color=color, label=label, linestyle=\"dashed\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [
