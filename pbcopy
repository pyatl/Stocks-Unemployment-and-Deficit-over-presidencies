    "deficit = pd.read_csv('./federal_deficit_trends.csv')[[\"year\", 'federal_deficit']]\n",
    "deficit = deficit.rename(columns={'year': \"Date\", 'federal_deficit': \"Deficit\"})\n",
    "deficit[\"Deficit\"] = -deficit[\"Deficit\"]\n",
    "deficit[\"Date\"] = deficit[\"Date\"].map(lambda x: dt(month=12, day=31, year=x))\n",
    "deficit = deficit.set_index(\"Date\")\n",
    "deficit.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {},
   "outputs": [],
   "source": [
    "bush_deficit_mask = (deficit.index >= bush_inaguration) & (deficit.index < obama_inaguration)\n",
    "obama_deficit_mask = (deficit.index >= obama_inaguration) & (deficit.index < trump_inaguration)\n",
    "trump_deficit_mask = (deficit.index >= trump_inaguration)\n",
    "covid_mask = (deficit.index >= covid_begins)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "deficit = pd.DataFrame(deficit)\n",
    "deficit['President'] = \"\"\n",
    "deficit[\"COVID\"] = covid_mask\n",
    "deficit.loc[bush_deficit_mask, \"President\"] = \"Bush\"\n",
    "deficit.loc[obama_deficit_mask, \"President\"] = \"Obama\"\n",
    "deficit.loc[trump_deficit_mask, \"President\"] = \"Trump\"\n",
    "deficit = deficit[deficit.index > bush_inaguration]\n",
    "deficit = deficit.reset_index()"
   ]
  },
